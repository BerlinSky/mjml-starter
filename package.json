    "babel-register": "^6.26.0",
    "es6-promisify": "^5.0.0",
    "eslint": "^4.5.0",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-config-prettier": "^2.6.0",
    "eslint-plugin-import": "^2.7.0",
    "lerna": "^2.0.0",
    "mjml": "4.0.3",
